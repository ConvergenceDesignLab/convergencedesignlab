{
  "name": "convergencedesignlab",
  "description": "Convergence Design Lab",
  "version": "1.0.0",
  "author": "Michael Hadley <mhadley@colum.edu>",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "dotenv": "^6.0.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.270",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-create-client-paths": "^1.0.3",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sass": "^1.0.15",
    "gh-pages": "^1.2.0",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "minimist": "^1.2.0",
    "modularscale-sass": "^3.0.4",
    "objectFitPolyfill": "^2.1.1",
    "react-autosize-textarea": "^3.0.3",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.3.1",
    "react-twitter-widgets": "^1.7.1",
    "striptags": "^2.0.0",
    "typeface-montserrat": "0.0.54",
    "typeface-open-sans": "0.0.54",
    "typi": "^3.1.2",
    "vinyl-fs": "^3.0.3",
    "vinyl-ftp": "^0.6.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "prettier": {
    "printWidth": 100
  },
  "scripts": {
    "test": "node ./scripts/deploy-ftp.js --src ./public/** --dest /usr/home/convergence/public_html/convergencedesignlab.org/new-site",
    "clean": "rm -rf public/* .cache",
    "build": "gatsby build --prefix-paths",
    "clean-build": "npm run clean && npm run build",
    "build:debug": "gatsby build --prefix-paths --noUglify",
    "gatsby": "gatsby",
    "dev": "gatsby develop --open",
    "push:ftp": "node ./scripts/deploy-ftp.js --src ./public/** --dest /usr/home/convergence/public_html/convergencedesignlab.org/new-site",
    "deploy:ftp": "set \"DEPLOY_TARGET=ftp\" && npm run clean-build && npm run push:ftp",
    "deploy:gh-pages": "set \"DEPLOY_TARGET=gh-pages\" && npm run clean-build && gh-pages --dist public --branch gh-pages",
    "deploy": "npm run deploy:gh-pages && npm run deploy:ftp"
  },
  "devDependencies": {
    "cross-env": "^5.1.6",
    "prettier": "^1.13.4"
  }
}
